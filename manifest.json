{
  "manifest_version": 2,
  "name": "Managed Containers",
  "version": "0.1.0",
  "incognito": "not_allowed",
  "permissions": [
    "<all_urls>",
    "cookies",
    "contextualIdentities",
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "browser_style": true,
    "default_title": "Managed Containers",
    "default_popup": "popup.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "managed-containers@stuartsmall.com"
    }
  }
}
